				<segment state="initial">
					<source>Download and install <pc dataRefEnd="id2" dataRefStart="id1" id="p1"><pc dataRefEnd="id4" dataRefStart="id3" id="p2">VSCode</pc></pc>.</source>
				</segment>
			</unit>
