				<originalData>
					<data id="id1">**</data>
					<data id="id2">**</data>
				</originalData>
				<segment state="initial">
					<source>Here is my recommendation for <pc dataRefEnd="id2" dataRefStart="id1" id="p1">someone brand new to Git</pc> who wants to get started right away:</source>
				</segment>
			</unit>
