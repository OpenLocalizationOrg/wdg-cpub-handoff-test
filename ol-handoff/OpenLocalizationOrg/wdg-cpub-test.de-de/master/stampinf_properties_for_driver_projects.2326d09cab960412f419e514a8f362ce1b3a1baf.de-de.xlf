					<data id="id3">&lt;strong&gt;</data>
					<data id="id4">&lt;/strong&gt; &lt;em&gt;</data>
					<data id="id5">&lt;/em&gt;</data>
				</originalData>
				<segment state="initial">
					<source>This setting is equivalent to specifying the <ph dataRef="id1" id="ph1" />Stampinf<ph dataRef="id2" id="ph2" /> option <ph dataRef="id3" id="ph3" />-c<ph dataRef="id4" id="ph4" />catalogfile<ph dataRef="id5" id="ph5" />.</source>
				</segment>
			</unit>
