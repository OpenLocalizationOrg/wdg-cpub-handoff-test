			<unit id="102" translate="yes">
				<originalData>
					<data id="id1">**</data>
					<data id="id2">**</data>
					<data id="id3">**</data>
					<data id="id4">**</data>
				</originalData>
				<segment state="initial">
					<source>When a project is selected in Solution Explorer, the <pc dataRefEnd="id2" dataRefStart="id1" id="p1">Properties</pc> dialog under the <pc dataRefEnd="id4" dataRefStart="id3" id="p2">Driver Signing</pc> node, displays two sections of properties:</source>
				</segment>
			</unit>
			<unit id="103" translate="yes">
