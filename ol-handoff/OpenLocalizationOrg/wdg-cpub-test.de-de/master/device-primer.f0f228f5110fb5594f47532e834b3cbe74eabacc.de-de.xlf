			<unit id="141" translate="yes">
				<segment state="initial">
					<source>Ultra-portable tablet computers are equipped with touchscreens, cameras, microphones, and accelerometers.</source>
				</segment>
			</unit>
			<unit id="142" translate="yes">
				<segment state="initial">
					<source>Tablet screen sizes usually range from 7” to 13.3”.</source>
				</segment>
			</unit>
			<unit id="143" translate="yes">
				<segment state="initial">
					<source>Screen sizes</source>
				</segment>
			</unit>
			<unit id="144" translate="yes">
