					<data id="id6">**</data>
					<data id="id7">[</data>
					<data id="id8">](connection-settings-ieak11-wizard.md)</data>
				</originalData>
				<segment state="initial">
					<source>Click <pc dataRefEnd="id2" dataRefStart="id1" id="p1">Next</pc> to go to the <pc dataRefEnd="id4" dataRefStart="id3" id="p2">Proxy Settings</pc> page or <pc dataRefEnd="id6" dataRefStart="id5" id="p3">Back</pc> to go to the <pc dataRefEnd="id8" dataRefStart="id7" id="p4">Connection Settings</pc> page.</source>
				</segment>
			</unit>
