				<originalData>
					<data id="id1">**</data>
					<data id="id2">**</data>
				</originalData>
				<segment state="initial">
					<source>Click <pc dataRefEnd="id2" dataRefStart="id1" id="p1">Confirm</pc> and your ads will typically start appearing on Windows or Windows Phone devices within a few hours!</source>
				</segment>
			</unit>
