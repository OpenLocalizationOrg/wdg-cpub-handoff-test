			<unit id="170" translate="yes">
				<originalData>
					<data id="id1">**</data>
					<data id="id2">**</data>
				</originalData>
				<segment state="initial">
					<source>Set a payment instrument for your ad campaign by clicking <pc dataRefEnd="id2" dataRefStart="id1" id="p1">Add new payment instrument</pc> and fill in your account details.</source>
				</segment>
			</unit>
			<unit id="171" translate="yes">
