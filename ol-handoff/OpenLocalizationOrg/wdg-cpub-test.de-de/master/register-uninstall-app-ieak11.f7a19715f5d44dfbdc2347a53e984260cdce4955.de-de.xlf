					<source>DisplayName</source>
				</segment>
			</unit>
			<unit id="114" translate="yes">
				<segment state="initial">
					<source>string</source>
				</segment>
			</unit>
			<unit id="115" translate="yes">
				<segment state="initial">
					<source>Friendly name for your uninstall app.</source>
				</segment>
			</unit>
			<unit id="116" translate="yes">
				<originalData>
					<data id="id1">**</data>
					<data id="id2">**</data>
					<data id="id3">**</data>
					<data id="id4">**</data>
				</originalData>
				<segment state="initial">
					<source>This name must match your <pc dataRefEnd="id2" dataRefStart="id1" id="p1">Uninstall Key</pc> in the <pc dataRefEnd="id4" dataRefStart="id3" id="p2">Add a Custom Component</pc> page of the Internet Explorer Customization Wizard 11.</source>
				</segment>
			</unit>
			<unit id="117" translate="yes">
