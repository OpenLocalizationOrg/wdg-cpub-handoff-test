				</originalData>
				<segment state="initial">
					<source>We call these <pc dataRefEnd="id2" dataRefStart="id1" id="p1">house ads</pc>.</source>
				</segment>
			</unit>
			<unit id="107" translate="yes">
				<originalData>
					<data id="id1">**</data>
