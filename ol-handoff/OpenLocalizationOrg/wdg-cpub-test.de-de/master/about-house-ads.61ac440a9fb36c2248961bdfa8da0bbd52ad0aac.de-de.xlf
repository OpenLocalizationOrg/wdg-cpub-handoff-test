			<unit id="101" translate="yes">
				<segment state="initial">
					<source>You can create an ad to promote one of your apps in your other apps, for free.</source>
				</segment>
			</unit>
			<unit id="102" translate="yes">
				<segment state="initial">
					<source>We call these house ads.</source>
				</segment>
			</unit>
