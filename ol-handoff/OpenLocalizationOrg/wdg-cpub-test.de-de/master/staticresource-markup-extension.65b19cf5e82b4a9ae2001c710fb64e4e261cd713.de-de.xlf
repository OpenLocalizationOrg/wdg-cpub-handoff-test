				<segment state="initial">
					<source><pc dataRefEnd="id2" dataRefStart="id1" id="p1">StaticResource</pc> takes one argument, which specifies the key for the requested resource.</source>
				</segment>
			</unit>
			<unit id="123" translate="yes">
