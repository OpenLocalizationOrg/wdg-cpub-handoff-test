		<group id="content">
			<unit id="101" translate="yes">
				<segment state="initial">
					<source>Search to see if a specific site already appears in your global Enterprise Mode site list.</source>
				</segment>
			</unit>
			<unit id="102" translate="yes">
				<segment state="initial">
					<source>Search your Enterprise Mode site list in the Enterprise Mode Site List Manager (Internet Explorer 11 for IT Pros)</source>
				</segment>
			</unit>
			<unit id="103" translate="yes">
				<segment state="initial">
					<source>Search your Enterprise Mode site list in the Enterprise Mode Site List Manager</source>
				</segment>
			</unit>
			<unit id="104" translate="yes">
				<segment state="initial">
					<source>Applies to:</source>
				</segment>
			</unit>
			<unit id="105" translate="yes">
				<segment state="initial">
					<source>Windows 10</source>
				</segment>
			</unit>
			<unit id="106" translate="yes">
				<segment state="initial">
					<source>Windows 8.1</source>
				</segment>
			</unit>
			<unit id="107" translate="yes">
				<segment state="initial">
					<source>Windows 7</source>
				</segment>
			</unit>
			<unit id="108" translate="yes">
				<segment state="initial">
					<source>Windows Server 2012 R2</source>
				</segment>
			</unit>
			<unit id="109" translate="yes">
				<segment state="initial">
					<source>Windows Server 2008 R2 with Service Pack 1 (SP1)</source>
				</segment>
			</unit>
			<unit id="110" translate="yes">
				<segment state="initial">
					<source>You can search to see if a specific site already appears in your global Enterprise Mode site list so you don’t try to add it again.</source>
				</segment>
			</unit>
			<unit id="111" translate="yes">
				<segment state="initial">
					<source>To search your compatibility list</source>
				</segment>
			</unit>
			<unit id="112" translate="yes">
				<originalData>
					<data id="id1">**</data>
					<data id="id2">**</data>
				</originalData>
				<segment state="initial">
					<source>From the Enterprise Mode Site List Manager, type part of the URL into the <pc dataRefEnd="id2" dataRefStart="id1" id="p1">Search</pc> box.</source>
				</segment>
			</unit>
			<unit id="113" translate="yes">
				<segment state="initial">
					<source>The search query searches all of the text.</source>
				</segment>
			</unit>
			<unit id="114" translate="yes">
				<originalData>
					<data id="id1">*</data>
					<data id="id2">*</data>
				</originalData>
				<segment state="initial">
					<source>For example, entering <pc dataRefEnd="id2" dataRefStart="id1" id="p1">“micro”</pc> will return results like, www.microsoft.com, microsoft.com, and microsoft.com/images.</source>
				</segment>
			</unit>
			<unit id="115" translate="yes">
				<segment state="initial">
					<source>Wildcard characters aren’t supported.</source>
				</segment>
			</unit>
