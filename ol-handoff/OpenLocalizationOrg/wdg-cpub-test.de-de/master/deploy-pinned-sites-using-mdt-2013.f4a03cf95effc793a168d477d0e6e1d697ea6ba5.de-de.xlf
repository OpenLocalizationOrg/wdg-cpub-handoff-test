					<data id="id2">**</data>
				</originalData>
				<segment state="initial">
					<source>You can add up to six links to the <pc dataRefEnd="id2" dataRefStart="id1" id="p1">TaskbarLinks</pc> item.</source>
				</segment>
			</unit>
