				<segment state="initial">
					<source>Note</source>
				</segment>
			</unit>
			<unit id="106" translate="yes">
				<segment state="initial">
					<source>IEAK 11 works in network environments, with or without Microsoft Active Directory.</source>
				</segment>
			</unit>
